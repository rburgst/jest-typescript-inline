{
  "private": true,
  "version": "0.0.0",
  "name": "example-typescript",
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "24.1.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
